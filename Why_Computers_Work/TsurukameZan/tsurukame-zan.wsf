<?xml version="1.0" encoding="utf-8" ?>
<package>
  <job>
    <runtime></runtime>
    <script language="VBScript">
    <![CDATA[
      For x = 0 To 10
          For y = 0 To 10
            a = x + y
            b = 2 * x + 4 * y
            If (a = 10) And (b = 32) Then
                MsgBox "鶴 = " & CStr(x) & ",亀 = " & CStr(y)
            End If
          Next
      Next
    ]]>
    </script>
  </job>
</package>
